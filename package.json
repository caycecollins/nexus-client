{
  "reactToolbox": {
    "include": [
      "AUTOCOMPLETE",
      "AVATAR",
      "BUTTON",
      "CARD",
      "CHECKBOX",
      "CHIP",
      "DATE_PICKER",
      "DIALOG",
      "DRAWER",
      "DROPDOWN",
      "INPUT",
      "LAYOUT",
      "LINK",
      "LIST",
      "MENU",
      "NAVIGATION",
      "OVERLAY",
      "PROGRESS_BAR",
      "RADIO",
      "RIPPLE",
      "SLIDER",
      "SNACKBAR",
      "SWITCH",
      "TABLE",
      "TABS",
      "TIME_PICKER",
      "TOOLTIP"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "color-accent": "var(--palette-pink-a200)",
      "color-accent-dark": "var(--palette-pink-700)",
      "color-primary-contrast": "var(--color-dark-contrast)",
      "color-accent-contrast": "var(--color-dark-contrast)"
    },
    "output": "dist/react-toolbox"
  },
  "name": "nexus-client",
  "version": "0.0.1",
  "description": "Front-end for Nexus Gaming",
  "scripts": {
    "build": "npm run clean && npm run build:dll && npm run build:i18n && npm run build:webpack",
    "build:dll": "webpack --config webpack.dll.babel.js --progress",
    "build:i18n": "babel-node task/localize.js --presets es2015,stage-0",
    "build:webpack": "webpack --config webpack.prod.babel.js --progress",
    "clean": "rimraf dist",
    "start": "webpack-dev-server --config webpack.dev.babel.js --hot --inline --progress",
    "start:full": "npm run clean && npm run build:dll && npm run build:i18n && npm run toolbox && webpack-dev-server --config webpack.dev.babel.js --hot --inline --progress",
    "start:optimized": "npm run clean && npm run build:dll && npm run build:i18n && npm run toolbox && webpack-dev-server --config webpack.dev.babel.js --hot --inline --optimize-minimize --progress",
    "test": "jest",
    "toolbox": "react-toolbox-themr"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "jest": {
    "notify": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "./src/test/setup",
    "testPathDirs": [
      "src"
    ],
    "testPathIgnorePatterns": [
      "/src/test/*"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/components/Main/index.js"
    ]
  },
  "license": "UNLICENSED",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cerebral": "next",
    "cerebral-provider-forms": "next",
    "cerebral-provider-http": "next",
    "cerebral-provider-storage": "next",
    "cerebral-router": "1.0.0-4-router-great-again",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "draft-js": "^0.10.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "function-tree": "next",
    "intl": "^1.2.5",
    "jest-cli": "^19.0.2",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.2",
    "polished": "^1.0.3",
    "postcss-cssnext": "^2.10.0",
    "postcss-each": "^0.9.3",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-mixins": "^5.4.1",
    "postcss-reporter": "^3.0.0",
    "prop-types": "^15.5.8",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-draft-wysiwyg": "^1.9.4",
    "react-hot-loader": "next",
    "react-icons": "^2.2.3",
    "react-intl": "^2.2.3",
    "react-material-responsive-grid": "^1.1.0",
    "react-medium-image-zoom": "^1.0.0",
    "react-style-proptype": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-toolbox": "2.0.0-beta.8",
    "react-toolbox-themr": "^1.0.2",
    "react-transition-group": "^1.1.1",
    "reactstrap": "^4.5.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "styled-components": "^1.4.5",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
