{
  "name": "nexus-api",
  "version": "0.0.1",
  "description": "Front-end for Nexus Gaming",
  "scripts": {
    "build": "npm run clean && npm run build:dll && npm run build:i18n && npm run build:webpack",
    "build:dll": "webpack --config webpack.dll.babel.js --progress",
    "build:i18n": "babel-node task/localize.js --presets es2015,stage-0",
    "build:webpack": "webpack --config webpack.prod.babel.js --progress",
    "clean": "rimraf dist",
    "start": "webpack-dev-server --config webpack.dev.babel.js --hot --inline --progress",
    "start:full": "npm run clean && npm run build:dll && npm run build:i18n && webpack-dev-server --config webpack.dev.babel.js --hot --inline --progress",
    "start:optimized": "npm run clean && npm run build:dll && npm run build:i18n && webpack-dev-server --config webpack.dev.babel.js --hot --inline --optimize-minimize --progress",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "jest": {
    "notify": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "./src/test/setup",
    "testPathDirs": [
      "src"
    ],
    "testPathIgnorePatterns": [
      "/src/test/*"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/components/Main/index.js"
    ]
  },
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "bootstrap": "4.0.0-alpha.6",
    "cerebral": "next",
    "cerebral-provider-http": "next",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "2.0.0-rc.2",
    "function-tree": "next",
    "intl": "^1.2.5",
    "jest-cli": "^18.1.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.5.0",
    "polished": "^1.0.2",
    "prop-types": "^15.5.4",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.5.1",
    "react-addons-transition-group": "^15.5.1",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "react-icons": "^2.2.3",
    "react-intl": "^2.2.3",
    "reactstrap": "^4.2.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "styled-components": "^1.4.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
